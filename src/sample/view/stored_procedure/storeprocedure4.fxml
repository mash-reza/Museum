<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="690.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.stored_procedure.Store4Controller">
    <children>
        <RadioButton layoutX="158.0" layoutY="14.0" mnemonicParsing="false" text="Painting" fx:id="paintingRadio" selected="true"/>
        <RadioButton layoutX="267.0" layoutY="14.0" mnemonicParsing="false" text="Sculpture" fx:id="sculptureRadio" selected="false"/>
        <Button fx:id="button" layoutX="358.0" layoutY="10.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="66.0" text="Get"/>
        <Label layoutX="14.0" layoutY="14.0" text="Get art objects by type"/>
        <TableView fx:id="tableView" layoutX="14.0" layoutY="61.0" prefHeight="633.0" prefWidth="1200.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <columns>
                <TableColumn prefWidth="75.0" text="Title" fx:id="title"/>
                <TableColumn prefWidth="75.0" text="Description" fx:id="description"/>
                <TableColumn prefWidth="75.0" text="Year" fx:id="year"/>
                <TableColumn prefWidth="75.0" text="Country" fx:id="country"/>
                <TableColumn prefWidth="75.0" text="Epoch" fx:id="epoch"/>
                <TableColumn prefWidth="75.0" text="Style" fx:id="style"/>
                <TableColumn prefWidth="75.0" text="Status" fx:id="status"/>
                <TableColumn prefWidth="75.0" text="Category" fx:id="category"/>
                <TableColumn prefWidth="75.0" text="Paint Type" fx:id="height"/>
                <TableColumn prefWidth="75.0" text="Material" fx:id="weight"/>
                <TableColumn visible="false" prefWidth="75.0" text="Column X" fx:id="sculptureType"/>

            </columns>
        </TableView>
    </children>
</AnchorPane>
